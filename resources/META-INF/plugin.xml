<idea-plugin>
  <id>com.ctrip.zt.gaok.plugin.easyCodePlugins</id>
  <name>EasyCodePlugins</name>
  <version>1.0</version>
  <vendor email="542385331@qq.com" url="http://www.ctrip.com/">Ctrip</vendor>

  <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

  <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="145.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="generateModelFieldFromString" class="actions.generateModelField.GenerateModelFieldAction" text="Generate Model Field From String"
            description="Generate Model Field From String">
      <add-to-group group-id="GenerateGroup" anchor="first"/>
    </action>
    <action id="GenerateJavaBeanBySting" class="actions.generateJavaBean.GenerateJavaBeanAction" text="Generate Java Bean By String"
            description="GenerateJavaBeanByString">
      <add-to-group group-id="NewGroup" anchor="first"/>
    </action>
  </actions>

</idea-plugin>